<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
                               "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>
	<data-sources />
	
	<form-beans>		
	</form-beans>
	
	<global-exceptions />
	
	<global-forwards>
		<forward name="fshowViewAllMessengerMessages" path="/jsp/messenger/viewAllMessengerMessages.jsp" />
	</global-forwards>
	
	<action-mappings>
		<action path="/messenger"
				parameter="method"
				scope="request"
				type="br.ufpe.cin.amadeus.amadeus_web.struts.action.MessengerActions">
		</action>
	</action-mappings>
	
	<controller nocache="true"  />
	<message-resources null="false"
		parameter="br.ufpe.cin.amadeus.amadeus_web.struts.messages.MessageResources" />
</struts-config>
